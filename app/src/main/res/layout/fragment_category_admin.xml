<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".screens.Fragment_category_admin">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/header_isip_11"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:background="@color/blue_white"
            android:gravity="center_vertical"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/isip_11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:text="ИСиП-11/Admin Category Homework"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="16sp" />
        </LinearLayout>

        <Button
            android:id="@+id/button_admin_math"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="Математика"
            app:layout_constraintEnd_toEndOf="@+id/header_isip_11"
            app:layout_constraintStart_toStartOf="@+id/header_isip_11"
            app:layout_constraintTop_toBottomOf="@+id/header_isip_11" />

        <Button
            android:id="@+id/button_admin_russian_lang"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Русский язык"
            app:layout_constraintEnd_toEndOf="@+id/button_admin_math"
            app:layout_constraintStart_toStartOf="@+id/button_admin_math"
            app:layout_constraintTop_toBottomOf="@+id/button_admin_math" />

        <Button
            android:id="@+id/button_admin_english_lang"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Английский язык"
            app:layout_constraintEnd_toEndOf="@+id/button_admin_russian_lang"
            app:layout_constraintStart_toStartOf="@+id/button_admin_russian_lang"
            app:layout_constraintTop_toBottomOf="@+id/button_admin_russian_lang" />

        <Button
            android:id="@+id/button_admin_phizicks"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Физика"
            app:layout_constraintEnd_toEndOf="@+id/button_admin_english_lang"
            app:layout_constraintStart_toStartOf="@+id/button_admin_english_lang"
            app:layout_constraintTop_toBottomOf="@+id/button_admin_english_lang" />

        <Button
            android:id="@+id/button_admin_information"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Информатика"
            app:layout_constraintEnd_toEndOf="@+id/button_admin_phizicks"
            app:layout_constraintStart_toStartOf="@+id/button_admin_phizicks"
            app:layout_constraintTop_toBottomOf="@+id/button_admin_phizicks" />

        <Button
            android:id="@+id/button_admin_litra"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Литература"
            app:layout_constraintEnd_toEndOf="@+id/button_admin_information"
            app:layout_constraintStart_toStartOf="@+id/button_admin_information"
            app:layout_constraintTop_toBottomOf="@+id/button_admin_information" />

        <Button
            android:id="@+id/button_admin_p_culture"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Физическая культура"
            app:layout_constraintEnd_toEndOf="@+id/button_admin_litra"
            app:layout_constraintStart_toStartOf="@+id/button_admin_litra"
            app:layout_constraintTop_toBottomOf="@+id/button_admin_litra" />

        <Button
            android:id="@+id/button_admin_prof"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Введение в профессию"
            app:layout_constraintEnd_toEndOf="@+id/button_admin_p_culture"
            app:layout_constraintStart_toStartOf="@+id/button_admin_p_culture"
            app:layout_constraintTop_toBottomOf="@+id/button_admin_p_culture" />

        <Button
            android:id="@+id/button_admin_history"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="История"
            app:layout_constraintEnd_toEndOf="@+id/button_admin_prof"
            app:layout_constraintStart_toStartOf="@+id/button_admin_prof"
            app:layout_constraintTop_toBottomOf="@+id/button_admin_prof" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>